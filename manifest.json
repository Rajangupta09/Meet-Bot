{
    "name": "G Bot",
    "description": "Build an Extension!",
    "version": "0.0.1",
    "manifest_version": 2,
    "icons": {"128": "img/meet_logo.svg"},
    "browser_action": {
      "default_icon": "img/meet_logo.svg",
      "default_popup": "popup.html"
    },"content_scripts": [{

      "matches": ["<all_urls>"],

      "js": ["js/contentScript.js"]

    }],
    "permissions": ["storage", "alarms", "tabs",
      "activeTab",
      "<all_urls>"],
    "background": {
    	"scripts": ["js/background.js"],
      "persistent": false

    }
  }
  